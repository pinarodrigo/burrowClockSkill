TEST "happy path"

  launch 
  user: "locate victoria"
  @name == "victoria"
  END

  launch 
  user: AMAZON.YesIntent
  END

  launch 
  user: AMAZON.NoIntent
  user: "find regina"
  @name == "regina"
  END

  launch 
  user: AMAZON.NoIntent
  user: "find viviana"
  @name == "viviana"
  END

  launch 
  user: AMAZON.NoIntent
  user: "find vivianar"
  @name == "vivianar"
  END


TEST "asking for help"
  launch 
  alexa: waitForName
  user: AMAZON.HelpIntent
  user: "locate regina"
  END

